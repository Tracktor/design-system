{
  "extends": "eslint-config-react",
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  },
  "overrides": [
    {
      "files": [
        "src/vite-env.d.ts"
      ],
      "rules": {
        "spaced-comment": "off"
      }
    },
    {
      "files": [
        "index.ts",
        "vite.config.ts"
      ],
      "rules": {
        "no-restricted-imports": "off"
      }
    }
  ]
}